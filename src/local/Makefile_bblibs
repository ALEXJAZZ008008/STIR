#
# $Id$
#


LOCAL_LIB_BUILDINGBLOCK= \
   ${WORKSPACE}/local/buildblock/${DEST}libbuildblock.a

LOCAL_BUILDBLOCK_REGISTRIES= \
    ${WORKSPACE}/local/buildblock/$(DEST)local_buildblock_registries.o


LOCAL_LIB_RECON_BUILDINGBLOCK= \
   ${WORKSPACE}/local/recon_buildblock/${DEST}librecon_buildblock.a

LOCAL_RECON_BUILDBLOCK_REGISTRIES= \
    ${WORKSPACE}/local/recon_buildblock/$(DEST)local_recon_buildblock_registries.o


LOCAL_LIB_SHAPE_BUILDINGBLOCK= \
   ${WORKSPACE}/local/Shape_buildblock/${DEST}libShape_buildblock.a

LOCAL_SHAPE_BUILDBLOCK_REGISTRIES= \
   ${WORKSPACE}/local/Shape_buildblock/${DEST}Shape_buildblock_registries.o


LOCAL_LIB_EVAL_BUILDINGBLOCK= \
   ${WORKSPACE}/local/eval_buildblock/${DEST}libeval_buildblock.a

LOCAL_LIB_MOTION= \
   ${WORKSPACE}/local/motion/${DEST}libmotion.a

LOCAL_LIB_LISTMODE= \
   ${WORKSPACE}/local/listmode/${DEST}liblistmode.a

LOCAL_MOTION_REGISTRIES= \
    ${WORKSPACE}/local/motion/$(DEST)local_motion_registries.o

$(LOCAL_LIB_BUILDINGBLOCK) $(LOCAL_BUILDBLOCK_REGISTRIES) : FORCE2
	cd $(WORKSPACE)/local/buildblock; mkdir -p $(DEST); $(MAKE) all;

$(LOCAL_LIB_RECON_BUILDINGBLOCK) $(LOCAL_RECON_BUILDBLOCK_REGISTRIES) : FORCE2
	cd $(WORKSPACE)/local/recon_buildblock; mkdir -p $(DEST); $(MAKE) all;

$(LOCAL_LIB_SHAPE_BUILDINGBLOCK) $(LOCAL_SHAPE_BUILDBLOCK_REGISTRIES): FORCE2
	cd $(WORKSPACE)/local/Shape_buildblock; mkdir -p $(DEST); $(MAKE) all;

$(LOCAL_LIB_EVAL_BUILDINGBLOCK) : FORCE2
	cd $(WORKSPACE)/local/eval_buildblock; mkdir -p $(DEST); $(MAKE) all;

$(LOCAL_LIB_MOTION) : FORCE2
	cd $(WORKSPACE)/local/motion; mkdir -p $(DEST); $(MAKE) all;

$(LOCAL_LIB_LISTMODE) : FORCE2
	cd $(WORKSPACE)/local/listmode; mkdir -p $(DEST); $(MAKE) all;

FORCE2:

.PHONY: FORCE2


LOCAL_BB_LIBS = \
	$(LOCAL_LIB_SHAPE_BUILDINGBLOCK) \
	$(LOCAL_LIB_EVAL_BUILDINGBLOCK) \
	$(LOCAL_LIB_MOTION) \
	$(LOCAL_LIB_LISTMODE) \
        $(LOCAL_LIB_BUILDINGBLOCK) \
	$(LOCAL_LIB_RECON_BUILDINGBLOCK) 

LOCAL_BB_REGISTRIES=$(LOCAL_BUILDBLOCK_REGISTRIES) \
	$(LOCAL_RECON_BUILDBLOCK_REGISTRIES) \
	$(LOCAL_SHAPE_BUILDBLOCK_REGISTRIES) \
	$(LOCAL_MOTION_REGISTRIES)

